class CASC {
    ctor(msg: string) {
        println("Hi! " + msg + "I'm CASC!")
    }

    fn pow(base: int): int {
        base * base
    }

    fn getNum(): int {
        return 1
    }

    fn main(arg: string[]) {
        b := CASC("ChAoS")

        for i from 0 to 10 {
            a := b.pow(i)
            println(a)
        }

        println(-b.getNum())
    }
}